{
  "rewrites": [
    { "source": "/", "destination": "/src/autentificacion/pages/index.html" },
    { "source": "/registrer", "destination": "/src/autentificacion/pages/registrer.html" },
    { "source": "/src/(.*)", "destination": "/src/$1" },
    { "source": "/public/(.*)", "destination": "/public/$1" }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}