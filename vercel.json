{
    "rewrites": [
      { "source": "/", "destination": "/src/autentificacion/pages/index.html" },
      { "source": "/registrer", "destination": "/src/pages/registrer.html" },
      { 
        "source": "/(.*)", 
        "destination": "/src/autentificacion/pages/index.html" 
      }
    ],
    "routes": [
      { 
        "src": "/public/(.*)", 
        "dest": "/public/$1" 
      },
      { 
        "src": "/src/(.*)", 
        "dest": "/src/$1" 
      }
    ],
    "buildCommand": "",
    "outputDirectory": "src/pages"
  }